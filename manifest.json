{
    "name": "Twitch Mod Chrome Extension",
    "description": "Adding things to make a mod's life easier.",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage","activeTab","scripting"],
    "action": {
        "default_popup":"popup/popup.html"
    },
    "options_page": "options/options.html"
  }